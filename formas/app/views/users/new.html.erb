

<%= form_for @user do |f| %>
  <p>
    <%= f.label :User %> <br>
    <%= f.text_field :username %>  
  </p>
  <p>
    <%= f.label :Email %> <br>
    <%= f.text_field :email %>  
  </p>
  <p>
    <%= f.label :Password %> <br>
    <%= f.password_field :password %>  
  </p>
  <%= f.submit "Create new User" %>
<% end %>



